{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./build_electron",
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ]
  },
  "include": ["src"],
  "exclude": ["src/renderer", "node_modules"]
}

{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "build_electron/main/electron.js",
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "scripts": {
    "start": "concurrently -k \"yarn start:react\" \"yarn start:electron\"",
    "start:react": "react-app-rewired start",
    "start:electron": "yarn tsc:electron && wait-on http://localhost:3000 && cross-env NODE_ENV=development electron ./build_electron/main/electron.js",
    "tsc:electron": "tsc --build tsconfig.electron.json",
    "build": "yarn build:react && yarn build:electron",
    "build:react": "yarn clean:react && react-app-rewired build",
    "build:electron": "yarn clean:electron && yarn tsc:electron && electron-builder",
    "clean:react": "rm -rf build",
    "clean:electron": "rm -rf build_electron dist",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typia": "^5.4.1",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "typescript": "5.3.2",
    "wait-on": "^7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
